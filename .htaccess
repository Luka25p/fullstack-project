RewriteEngine on

RewriteCond %{REQUEST_FILENAME} !-d

RewriteCond %{REQUEST_FILENAME}\.php -f

RewriteRule ^([a-zA-Z0-9_-]+)$ $1.php [L,NC]

RewriteRule ^drawingsMore/([0-9]+)/([0-9a-zA-Z_-]+) drawingsMore.php?id=$1&name=$2 [NC]

RewriteRule ^designsMore/([0-9]+)/([0-9a-zA-Z_-]+) designsMore.php?id=$1&name=$2 [NC]

